[*.cs]

# CS8632: La anotación para tipos de referencia que aceptan valores NULL solo debe usarse en el código dentro de un contexto de anotaciones "#nullable".
dotnet_diagnostic.CS8632.severity = silent

# CS0108: El miembro oculta el miembro heredado. Falta una contraseña nueva
dotnet_diagnostic.CS0108.severity = silent

# CS1998: El método asincrónico carece de operadores "await" y se ejecutará de forma sincrónica
dotnet_diagnostic.CS1998.severity = silent
